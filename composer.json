{
	"name": "manychois/wp-wpx",
	"description": "A utility library for overriding WordPress default HTML output easily",
	"keywords": [ "wordpress" ],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Siu Pang Tommy Choi",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.0",
		"manychois/views": ">=0.1.0",
		"ivopetkov/html5-dom-document-php": ">=0.6.1"
	},
	"require-dev": {
		"php": ">=7.0",
		"phpunit/phpunit": "6.*",
		"johnpbloch/wordpress-core": "4.9.5"
	},
	"autoload": {
		"psr-4": { "Manychois\\Wpx\\": "src/" }
	},
	"autoload-dev": {
		"psr-4": { "Manychois\\Wpx\\Tests\\": "tests/" }
	},
	"scripts": {
		"unit-test":  "vendor/bin/phpunit"
	}
}